{
  "name": "piacere-domlr",
  "displayName": "PIACERE DOMLR",
  "description": "Syntax Highlighting for PIACERE DOMLR files.",
  "version": "1.2.0",
  "repository": "https://github.com/andreafra/piacere-domlr",
  "publisher": "andreafra",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "domlr",
      "aliases": ["DOMLR", "domlr"],
      "extensions": [".domlr"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "domlr",
      "scopeName": "source.domlr",
      "path": "./syntaxes/domlr.tmLanguage.json"
    }]
  }
}
